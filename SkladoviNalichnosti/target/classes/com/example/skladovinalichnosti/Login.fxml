<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: FEF1E6;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.skladovinalichnosti.Login">
    <children>
        <Button fx:id="LoginFXID" layoutX="218.0" layoutY="252.0" mnemonicParsing="false" onAction="#LoginOnAction" style="-fx-background-color: FFB085;" text="Вход">
         <font>
            <Font name="Bookman Old Style" size="14.0" />
         </font></Button>
      <PasswordField fx:id="PassFXID" layoutX="226.0" layoutY="200.0" onAction="#PassOnAction" promptText="Парола" style="-fx-background-color: F9D5A7;">
         <font>
            <Font name="Bookman Old Style" size="12.0" />
         </font></PasswordField>
      <TextField fx:id="ImailFXID" layoutX="227.0" layoutY="127.0" onAction="#ImailOnAction" promptText="Имейл" style="-fx-background-color: F9D5A7;">
         <font>
            <Font name="Bookman Old Style" size="12.0" />
         </font></TextField>
      <Button fx:id="RegisterFXID" layoutX="281.0" layoutY="252.0" mnemonicParsing="false" onAction="#RegisterOnAction" style="-fx-background-color: FFB085;" text="Регистрация">
         <font>
            <Font name="Bookman Old Style" size="14.0" />
         </font></Button>
      <Label fx:id="LabelFXID" layoutX="142.0" layoutY="234.0" prefHeight="18.0" prefWidth="322.0" text=" " />
    </children>
</AnchorPane>
