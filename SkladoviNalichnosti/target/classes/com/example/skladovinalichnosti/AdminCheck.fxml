<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: FEF1E6;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.skladovinalichnosti.AdminCheck">
   <children>
      <Button fx:id="ContinueFXID" layoutX="268.0" layoutY="290.0" mnemonicParsing="false" onAction="#ContinueOnAction" style="-fx-background-color: FFB085;" text="Напред">
         <font>
            <Font name="Bookman Old Style" size="14.0" />
         </font></Button>
      <PasswordField fx:id="PassFXID" layoutX="226.0" layoutY="187.0" onAction="#PassOnAction" promptText="Парола" style="-fx-background-color: F9D5A7;">
         <font>
            <Font name="Bookman Old Style" size="12.0" />
         </font></PasswordField>
      <TextField fx:id="NameFXID" layoutX="226.0" layoutY="114.0" onAction="#NameOnAction" promptText="Име" style="-fx-background-color: F9D5A7;">
         <font>
            <Font name="Bookman Old Style" size="12.0" />
         </font></TextField>
      <Button fx:id="BackFXID" layoutX="548.0" layoutY="2.0" mnemonicParsing="false" onAction="#BackOnAction" style="-fx-background-color: FFB085;" text="Назад">
         <font>
            <Font name="Bookman Old Style" size="14.0" />
         </font></Button>
      <Label fx:id="LabelFXID" layoutX="273.0" layoutY="243.0" />
   </children>
</AnchorPane>
